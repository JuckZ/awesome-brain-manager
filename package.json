{
  "name": "awesome-brain-manager",
  "version": "1.3.5",
  "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
  "main": "main.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{src,__mocks__,bin}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "commit": "git-cz",
    "format:check": "prettier --list-different {src,__mocks__,bin}/**/*.ts",
    "format:write": "prettier --write {src,__mocks__,bin}/**/*.ts",
    "start:help": "ts-node ./src/bin/order.bin.ts -h",
    "start:example": "ts-node ./src/bin/order.bin.ts pizza beer",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "report:coverage": "npm run test:coverage && codecov",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "lint": "eslint ./src --fix && tsc --noEmit --pretty && svelte-check",
    "semantic-release": "semantic-release",
    "version": "node version-bump.mjs",
    "prepare": "husky install"
  },
  "bin": {
    "example": "./dest/bin/order.bin.ts"
  },
  "files": [
    "dest"
  ],
  "keywords": [
    "reminder",
    "notification",
    "ntfy",
    "awesome-brain-manager",
    "Obsidian-plugin",
    "Obsidian"
  ],
  "author": "Juck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JuckZ/awesome-brain-manager/issues"
  },
  "homepage": "https://github.com/JuckZ/awesome-brain-manager#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@commitlint/prompt": "^17.2.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.6",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@the_tree/esbuild-plugin-vue3": "^0.3.1",
    "@tsconfig/svelte": "^3.0.0",
    "@types/jest": "^29.2.2",
    "@types/node": "^16.11.6",
    "@types/sql.js": "^1.4.4",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "codecov": "^3.8.3",
    "commitizen": "^4.2.5",
    "dotenv": "^16.0.3",
    "electron": "^21.2.3",
    "esbuild": "0.14.47",
    "esbuild-copy-static-files": "^0.1.0",
    "esbuild-plugin-vue-next": "^0.1.4",
    "esbuild-plugin-wat": "^0.2.7",
    "esbuild-sass-plugin": "^2.4.0",
    "esbuild-svelte": "^0.7.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "moment": "^2.29.1",
    "obsidian": "^1.1.1",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5",
    "svelte-check": "^2.9.2",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.7",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@codemirror/language": "^6.4.0",
    "@codemirror/state": "^6.2.0",
    "@codemirror/view": "^6.7.3",
    "@vicons/ionicons5": "^0.12.0",
    "axios": "^1.1.3",
    "boxen": "^5.1.2",
    "chart.js": "^4.2.0",
    "commander": "^9.4.1",
    "cursor-effects": "^1.0.8",
    "jsonfile": "^6.1.0",
    "naive-ui": "^2.34.3",
    "obsidian-daily-notes-interface": "^0.9.4",
    "party-js": "^2.2.0",
    "rrule": "^2.7.1",
    "sql.js": "^1.8.0",
    "svelte": "^3.53.1",
    "vue": "^3.2.45",
    "vue3-radial-progress": "^1.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:JuckZ/awesome-brain-manager.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  }
}
